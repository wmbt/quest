<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="HeartBeatSec" value="40"/>
    <add key="QuestId" value="1"/>
    <add key="ServerIP" value="192.168.1.200"/>
    <add key="ListenCommandPort" value="1451"/>
    <add key="SendToCommandPort" value="1451"/>
    <add key="KeyWindowInterval" value="90"/>
    <add key="QuestDurationMin" value="60"/>
    <add key="BackgroundMusicVolume" value="1"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  <system.serviceModel>
    <bindings>
      <wsDualHttpBinding>
        <binding  name="WSDualHttpBinding_IQuestService" maxReceivedMessageSize="20971520" openTimeout="00:00:10" closeTimeout="00:00:10" sendTimeout="00:00:20" maxBufferPoolSize="209715200">
          <security mode="None"/>
        </binding>
      </wsDualHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://192.168.1.200:8733/Design_Time_Addresses/QuestServer/QuestService/" binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_IQuestService" contract="NetworkService.IQuestService" name="WSDualHttpBinding_IQuestService">
        <identity>
          <!--<dns value="localhost"/>-->
        </identity>
      </endpoint>
    </client>
  </system.serviceModel>
</configuration>
