    <system.serviceModel>
      <bindings>
        <wsDualHttpBinding>
          <binding openTimeout="00:00:10" closeTimeout="00:00:10" sendTimeout="00:00:20" receiveTimeout="00:00:20" maxBufferPoolSize="209715200">
            <security mode="None">
              <message clientCredentialType="None" negotiateServiceCredential="false"/>
            </security>
          </binding>
        </wsDualHttpBinding>
      </bindings>  
      <behaviors>
            <serviceBehaviors>
                <behavior name="">
                    <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true"/>
                    <serviceDebug includeExceptionDetailInFaults="true"/>
                    <serviceThrottling maxConcurrentCalls="100" maxConcurrentInstances="100" maxConcurrentSessions="100"/>
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <service name="QuestServer.NetworkService.QuestService">
              <endpoint address="" binding="wsDualHttpBinding" contract="QuestService.IQuestService">
                    <identity>
                        <dns value="localhost"/>
                    </identity>
                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
                <host>
                    <baseAddresses>
                      <add baseAddress="http://192.168.1.200:8733/Design_Time_Addresses/QuestServer/QuestService/"/>
                      <!--<add baseAddress="http://192.168.6.2:8733/Design_Time_Addresses/QuestServer/QuestService/"/>-->
                      <!--<add baseAddress="http://10.189.214.60:8733/Design_Time_Addresses/QuestServer/QuestService/"/>-->
                    </baseAddresses>
                </host>
            </service>
        </services>
    </system.serviceModel>
  <system.net>
    <connectionManagement>
      <add address="*" maxconnection="100" />
    </connectionManagement>
  </system.net>
    <startup>
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
</configuration>
