            </serviceBehaviors>
        </behaviors>
        <services>
            <service name="QuestServer.NetworkService.QuestService">
              <endpoint address="" binding="wsDualHttpBinding" contract="QuestService.IQuestService">
                    <identity>
                        <dns value="localhost"/>
                    </identity>
                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
                <host>
                    <baseAddresses>
                        <add baseAddress="http://192.168.1.200:8733/Design_Time_Addresses/QuestServer/QuestService/"/>
                      <!--<add baseAddress="http://10.189.214.60:8733/Design_Time_Addresses/QuestServer/QuestService/"/>-->
